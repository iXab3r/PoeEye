<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:PoeBud.Converters"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <converters:PoeSolutionToHumanReadableFormat x:Key="PoeSolutionToHumanReadableFormatConverterKey" />

    <converters:BooleanToObjectConverter x:Key="BooleanToSemitransparentConverter">
        <converters:BooleanToObjectConverter.FalseValue>
            <system:Double>
                0.4
            </system:Double>
        </converters:BooleanToObjectConverter.FalseValue>
        <converters:BooleanToObjectConverter.TrueValue>
            <system:Double>
                1
            </system:Double>
        </converters:BooleanToObjectConverter.TrueValue>
    </converters:BooleanToObjectConverter>

    <converters:BooleanToObjectConverter x:Key="BoolInvertConverter">
        <converters:BooleanToObjectConverter.FalseValue>
            <system:Boolean>
                True
            </system:Boolean>
        </converters:BooleanToObjectConverter.FalseValue>
        <converters:BooleanToObjectConverter.TrueValue>
            <system:Boolean>
                False
            </system:Boolean>
        </converters:BooleanToObjectConverter.TrueValue>

    </converters:BooleanToObjectConverter>

</ResourceDictionary>