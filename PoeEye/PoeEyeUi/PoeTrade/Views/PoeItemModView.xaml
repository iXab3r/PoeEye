<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:PoeEyeUi.PoeTrade.ViewModels"
                    xmlns:poeEyeUi="clr-namespace:PoeEyeUi"
                    xmlns:common="clr-namespace:PoeShared.Common;assembly=PoeShared"
                    xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls">

    <DataTemplate DataType="{x:Type common:PoeItemMod}">
        <ContentControl>
            <TextBlock Text="{Binding Name}" Style="{StaticResource MetroTextBlock}" FontSize="15"/>
        </ContentControl>
    </DataTemplate>
</ResourceDictionary>