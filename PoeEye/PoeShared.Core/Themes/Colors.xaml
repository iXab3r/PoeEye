    <SolidColorBrush x:Key="MouseOverAccentBrushKey" Color="#80d8ff"></SolidColorBrush>
    
    <Color x:Key="PoeShallow">#11FFFFFF</Color>
    <Color x:Key="PoeFontColor">#FFFEEAC5</Color>
    <Color x:Key="PoeBgColor">#AA030303</Color>
